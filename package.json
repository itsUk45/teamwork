{
  "name": "teamwork",
  "version": "1.0.0",
  "description": "Teamwork is an​ internal social network for organizations’ employees. The goal of this application is to facilitate more interaction between colleagues and facilitate team bonding",
  "main": "app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node ./api/v1/config/prod",
    "build": "babel app.js -d lib",
    "dev": "nodemon --exec babel-node ./app.js",
    "lint": "eslint",
    "test": "mocha --require @babel/register ./api/v1/tests ",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "author": "uk45",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "form-data": "^2.5.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1"
  }
}
